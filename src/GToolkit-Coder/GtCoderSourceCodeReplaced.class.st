Class {
	#name : #GtCoderSourceCodeReplaced,
	#superclass : #GtSourceCoderAnnouncement,
	#instVars : [
		'sourceCode'
	],
	#category : #'GToolkit-Coder-Event'
}

{ #category : #accessing }
GtCoderSourceCodeReplaced >> sourceCode [

	^ sourceCode
]

{ #category : #accessing }
GtCoderSourceCodeReplaced >> sourceCode: anObject [

	sourceCode := anObject
]
