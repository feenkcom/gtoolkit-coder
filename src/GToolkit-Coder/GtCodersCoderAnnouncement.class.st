Class {
	#name : #GtCodersCoderAnnouncement,
	#superclass : #GtCodersAnnouncement,
	#instVars : [
		'coder'
	],
	#category : 'GToolkit-Coder-Event'
}

{ #category : #accessing }
GtCodersCoderAnnouncement >> coder [
	^ coder
]

{ #category : #accessing }
GtCodersCoderAnnouncement >> coder: anObject [
	coder := anObject
]
